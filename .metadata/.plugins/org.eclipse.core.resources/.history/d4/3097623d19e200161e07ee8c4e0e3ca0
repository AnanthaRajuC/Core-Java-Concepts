package io.github.anantharajuc;

public class SwitchStatement 
{
	public static void main(String[] args) 
	{
		int age = 21;
		
		switch (age)  
        {  
            case 18: System.out.println( "age 18" );  
                	 break;  
            case 19:  
                System.out.println( "age 19" );  
                break;  
            default:  
                System.out.println( "not matched" );  
                break;  
        }  
	
	}
}
